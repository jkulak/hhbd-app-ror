<h1>New band_membership</h1>

<% form_for(@band_membership) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :artist_id %><br />
    <%= f.text_field :artist_id %>
  </p>
  <p>
    <%= f.label :band_id %><br />
    <%= f.text_field :band_id %>
  </p>
  <p>
    <%= f.label :joined %><br />
    <%= f.date_select :joined %>
  </p>
  <p>
    <%= f.label :left %><br />
    <%= f.date_select :left %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.label :updated_by %><br />
    <%= f.text_field :updated_by %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', band_memberships_path %>