<ul class="tracklist">
<% object.each do |song| %>
	<li>(<%= song.track_appearances[0].as_number %>) <%= song_linking_name(song) %> (<%= convert_seconds_to_time(song.length) %>)</li>
	<% if admin? %>
		<li class="admin">Admin: <%= link_to 'Edytuj', edit_song_path(song) %> |
			<%= link_to 'UsuÅ„', song, :confirm => 'Na pewno?', :method => :delete %>
		</li>
	<% end %>
<% end %>
</ul>